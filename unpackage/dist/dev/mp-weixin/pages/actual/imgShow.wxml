<view class="container"><block wx:if="{{shici_conf_data.length>0}}"><view class="img-menu"><block wx:for="{{shici_conf_data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['menuToggle',['$0'],[[['shici_conf_data','',index]]]]]]]}}" class="{{['img-menu__list',(currentMenu.name===item.name)?'active':'']}}" bindtap="__e"><text class="menu__list-item">{{item.name}}</text></view></block></view></block><block wx:if="{{radarEle=='雷达'}}"><view class="orientation-container"></view></block><block wx:if="{{listData.length>0}}"><view class="img-main"><view class="img-main__time"><text>{{"更新时间："+curr_img_time}}</text></view><view class="img-main__imgs"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['touchmove',[['touchMoveHandle',['$event']]]],['touchstart',[['touchstart',['$event']]]],['touchend',[['touchend',['$event']]]]]}}" class="{{['main-imgs',(imgIndex==index)?'on':'']}}" bindtouchmove="__e" bindtouchstart="__e" bindtouchend="__e"><image class="main-imgs_img" style="{{'overflow:'+('scroll')+';'+('width:'+(setData.scaleWidth+'px')+';')+('height:'+(setData.scaleHeight+'px')+';')}}" mode="scaleToFill" src="{{imgBaseUrl+item.File_URL}}"></image></view></block></view><view class="img-main__control-wrap"><view class="img-main__control"><view data-event-opts="{{[['tap',[['autoPlay',['$event']]]]]}}" class="control-play" bindtap="__e"><block wx:if="{{isAutoPlay}}"><image src="../../static/images/actual/zanting.png"></image></block><block wx:else><image src="../../static/images/actual/bofang.png"></image></block></view><view class="control-slider"><pp-slider vue-id="5065a4ee-1" value="{{imgIndex}}" activeColor="rgb(0, 122, 255)" lineSize="8" min="0" max="{{imgLastIndex}}" step="1" data-event-opts="{{[['^change',[['sliderChange']]]]}}" bind:change="__e" bind:__l="__l"></pp-slider></view></view></view></view></block><block wx:else><view class="noData"><image src="/static/images/noData.png" mode="heightFix"></image><text>暂无数据！</text></view></block></view>
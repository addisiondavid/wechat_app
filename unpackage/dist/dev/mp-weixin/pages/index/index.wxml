<view class="container"><block wx:if="{{newEarlyWarnData!=''}}"><view class="con-box-notices"><uni-notice-bar vue-id="8dd740cc-1" speed="50" showIcon="true" scrollable="true" single="true" text="{{newEarlyWarnData}}" bind:__l="__l"></uni-notice-bar></view></block><block wx:if="{{basic}}"><view data-event-opts="{{[['tap',[['goWeather',['$event']]]]]}}" class="city-weather_current" bindtap="__e"><view class="row-inline city-weather_current_city"><text class="current__city">{{basic.Station_Name}}</text><text class="iconfont icon-address"></text><block wx:if="{{basic}}"><text class="current__title">{{''+(basic['TEM_Max_24h']?$root.m0:'-')+''}}<text class="current__title-symbol">℃</text>{{'/'+(basic['TEM_Min_24h']?$root.m1:'-')+''}}<text class="current__title-symbol">℃</text></text></block><block wx:else><text>-℃/-℃</text></block></view><view class="row-inline city-weather_current_tianqi"><view class="current_tianqi"><image class="current__img" src="{{basic.img}}"></image><text class="current__title">{{weatherName}}</text></view><view class="current_air"><view class="air-aqi"><text class="air_title">AQI等级：</text><view class="air-aqi__level" style="{{'background-color:'+(levelColor||'#ffffff')+';'}}"></view></view></view></view></view></block><view class="con-box con-box-products"><view class="box-header"><view class="box-header__title"><view class="header__title-tip"><image src="/static/images/shouye/juece-tip.png"></image></view><text class="header__title-text">信息专报</text></view><text data-event-opts="{{[['tap',[['goProductTabView',['$event']]]]]}}" class="box-header__more" bindtap="__e">查看全部>></text></view><view class="box-container"><block wx:if="{{newProductList.length>0}}"><view class="box-container__list" animation="{{animationData}}"><block wx:for="{{newProductList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getToDetail',['$0'],[[['newProductList','',index]]]]]]]}}" class="list_item" bindtap="__e"><image class="list_item__img" src="/static/images/shouye/laba.png"></image><text class="list_item__title">{{item.RETAIN1}}</text></view></block></view></block><block wx:else><view class="noData"><image src="/static/images/noData.png" mode="heightFix"></image><text>暂无数据！</text></view></block></view></view><view class="con-box con-box-actual"><view class="box-header"><view class="box-header__title"><view class="header__title-tip"><image src="/static/images/shouye/shikuang-tip.png"></image></view><text class="header__title-text">实况监测</text></view></view><view class="box-container"><block wx:for="{{subMenu.actual}}" wx:for-item="item" wx:for-index="index" wx:key="ele"><view data-event-opts="{{[['tap',[['getRouterView',['actual','$0'],[[['subMenu.actual','ele',item.ele]]]]]]]}}" class="subMenu-item" style="{{(subMenuStyle)}}" bindtap="__e"><view class="img"><image src="{{item.icon}}" mode="heightFix"></image></view><text class="name">{{item.name}}</text></view></block></view></view><view class="con-box con-box-forecast"><view class="box-header"><view class="box-header"><view class="box-header__title"><view class="header__title-tip"><image src="/static/images/shouye/yubao-tip.png"></image></view><text class="header__title-text">预警预报</text></view></view></view><view class="box-container"><block wx:for="{{subMenu.forecast}}" wx:for-item="item" wx:for-index="index" wx:key="ele"><view data-event-opts="{{[['tap',[['getRouterView',['forecast','$0'],[[['subMenu.forecast','ele',item.ele]]]]]]]}}" class="subMenu-item" style="{{(subMenuStyle)}}" bindtap="__e"><view class="img"><image src="{{item.icon}}" mode="heightFix"></image></view><text class="name">{{item.name}}</text></view></block></view></view></view>